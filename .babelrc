{
    "presets": [
        "env",
        "react",
        "es2015"
    ],
    "plugins": [
        "transform-async-to-generator",
        ["transform-runtime", {
          "helpers": false,
          "polyfill": false,
          "regenerator": true }],
        "transform-class-properties",
        "transform-object-rest-spread"
    ]
}